@using ZP50.Core.CentroAscolto;
@model DichiarazioneIsee
<fieldset>
    <legend>Dichiarazione ISEE</legend>
    <div class="form-row">

        <div class="form-group col">
            @Html.LabelFor(model => model.ValoreIndicatore)

            @Html.EditorFor(model => model.ValoreIndicatore, new { htmlAttributes = new { @class = "form-control-plaintext" } })
            @Html.ValidationMessageFor(model => model.ValoreIndicatore, "", new { @class = "text-danger" })

        </div>
        <div class="form-group col">
            @Html.LabelFor(model => model.DataScadenza)

            @Html.EditorFor(model => model.DataScadenza, new { htmlAttributes = new { @class = "form-control-plaintext" } })
            @Html.ValidationMessageFor(model => model.DataScadenza, "", new { @class = "text-danger" })
        </div>
    </div>
</fieldset>

